{
  "name": "plane-rectangular-jp",
  "version": "1.0.2",
  "description": "Converter of {lng, lat} <-> {x, y} (Plane rectangular coordinate system)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json --fix",
    "precommit": "lint-staged",
    "prepare": "npm run build"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sw1227/plane-rectangular-jp.git"
  },
  "keywords": [],
  "author": "Shingo Watanabe",
  "license": "ISC",
  "devDependencies": {
    "prettier": "^2.6.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.6.4"
  }
}
